<h1 class = "marginAll">Usages</h1>

<table class="table">
	<tr>
		<th>User</th>
		<th>Feature</th>
		<th>No of Units Used</th>
		<th>Date</th>
	</tr>
	<% @usages.each do | usage | %>
	<tr>
		<td><%= usage.subscription.user.name %></td>
		<td><%= usage.feature.name %></td>
		<td><%= usage.no_of_units_used %></td>
		<td><%= usage.created_at %></td>
	</tr>	
	<% end %>
</table>
